{
  "name": "credit-card-type",
  "version": "10.0.2",
  "description": "A library for determining credit card type",
  "main": "/dist/cjs/index.js",
  "module": "/dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "lint": "eslint --ext js,ts .",
    "posttest": "npm run lint",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest",
    "test:integration": "npm run test:integration:cjs && npm run test:integration:esm && npm run test:integration:imports",
    "test:integration:cjs": "node test/integration/commonjs.test.cjs",
    "test:integration:esm": "node test/integration/esm.test.mjs",
    "test:integration:imports": "node test/integration/esm-mixed-imports.test.mjs",
    "prepublishOnly": "npm run build",
    "prebuild": "prettier --write .",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:braintree/credit-card-type"
  },
  "homepage": "https://github.com/braintree/credit-card-type",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "eslint": "^8.47.0",
    "eslint-config-braintree": "^6.0.0-typescript-prep-rc.2",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
